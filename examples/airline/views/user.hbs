<h1>Welcome, {{user.username}}.</h1>
<p>History:</p>


<table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
    <thead>
    <tr>
        <th>Date</th>
        <th>URL</th>
        <th>Method</th>
    </tr>
    </thead>
    <tbody>
    {{#each history}}
        <tr>
            <td>{{date}}</td>
            <td>{{url}}</td>
            <td>{{method}}</td>
        </tr>
    {{/each}}

    </tbody>
</table>